{
  "name": "qr-universalscanner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css:scss": "node-sass --output-style compressed -o dist src/scss",
    "css:autoprefixer": "postcss -u autoprefixer -r dist/*.css",
    "build:css": "npm run css:scss && npm run css:autoprefixer",
    "build:html": "posthtml -c posthtml.json",
    "build:js": "webpack --mode=production",
    "build": "run-p build:*",
    "watch:css": "onchange \"src/scss\" -- npm run build:css",
    "watch:js": "onchange \"src/js\" -- webpack --mode=development",
    "watch:html": "onchange \"src/views\" -- npm run build:html",
    "watch": "run-p serve watch:*",
    "serve": "browser-sync start --server \"dist\" --files \"dist\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ContentsViewer/QR-UniversalScanner.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ContentsViewer/QR-UniversalScanner/issues"
  },
  "homepage": "https://github.com/ContentsViewer/QR-UniversalScanner#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "node-sass": "^6.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@popperjs/core": "^2.9.2",
    "autoprefixer": "^10.2.6",
    "bootstrap": "^5.0.1",
    "bootstrap-icons": "^1.5.0",
    "browser-sync": "^2.26.14",
    "htmlnano": "^1.0.0",
    "jquery": "^3.6.0",
    "jsqr": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "popper.js": "^1.16.1",
    "postcss-cli": "^8.3.1",
    "posthtml": "^0.16.4",
    "posthtml-cli": "^0.10.0",
    "posthtml-modules": "^0.7.4"
  }
}
